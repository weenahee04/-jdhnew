# Gemini API Key
# Get your API key from: https://aistudio.google.com/app/apikey
# Copy this file to .env.local and add your API key
GEMINI_API_KEY=your_gemini_api_key_here

# Solana / Helius
# Example: https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_key_here
SOLANA_CLUSTER=mainnet-beta

# Jupiter (Swap)
JUPITER_BASE_URL=https://quote-api.jup.ag

# Wallet API Backend (Optional - for using the new backend API server)
VITE_WALLET_API_URL=http://localhost:3001
VITE_USE_WALLET_API=false
VITE_WALLET_API_KEY=your-api-key-optional

